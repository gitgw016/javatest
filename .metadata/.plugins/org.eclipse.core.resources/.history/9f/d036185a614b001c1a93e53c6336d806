package second;

import java.net.URL;
import java.util.ResourceBundle;

import javafx.fxml.FXML;
import javafx.fxml.Initializable;
import javafx.scene.control.Button;

public class SecondController implements Initializable {
	
	@FXML private Button btn1, btn2;
	
	@Override
	public void initialize(URL location, ResourceBundle resources) {
		btn1.setText("_Main");
		btn1.setMnemonicParsing(true);
		btn1.setOnAction(event->{
			System.out.println("Main으로 가자");
			FXMLLoader loader = new FXMLLoader(getClass().getResource("../main/Root.fxml"));
		});
		
		btn2.setText("_A");
		btn2.setMnemonicParsing(true);
		btn2.setOnAction(event->{
			System.out.println("btn2 click");
		});
	}

}
